    <%= javascript_include_tag "jquery-1.11.1", "bootstrap", "jquery.vegas.min", "jquery.easing.min", "jquery.fancybox", "jquery.isotope", "appear.min", "animations.min", "custom-solid" %>

  </body>
</html>
